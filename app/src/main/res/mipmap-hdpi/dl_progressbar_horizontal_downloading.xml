<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@*android:id/background" android:drawable="@drawable/dl_progressbar_background">
        <shape>
            <corners android:radius="0.0dip" />
            <gradient android:startColor="#ff9d9e9d" android:endColor="#ff747674" android:angle="270.0" android:centerY="0.75" />
        </shape>
    </item>
    <item android:id="@*android:id/secondaryProgress" android:drawable="@drawable/dl_progressbar_downloading">
        <clip>
            <shape>
                <corners android:radius="0.0dip" />
                <gradient android:startColor="#80ff9900" android:endColor="#a0ff9900" android:angle="270.0" android:centerY="0.75" />
            </shape>
        </clip>
    </item>
    <item android:id="@*android:id/progress" android:drawable="@drawable/dl_progressbar_downloading">
        <clip>
            <shape>
                <corners android:radius="0.0dip" />
                <gradient android:startColor="#ff30ee00" android:endColor="#ff30ee00" android:angle="270.0" android:centerY="0.75" />
            </shape>
        </clip>
    </item>
</layer-list>