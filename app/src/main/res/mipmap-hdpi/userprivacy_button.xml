<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <gradient android:startColor="@color/privacy_button_focus" android:endColor="@color/privacy_button_focus" android:angle="270.0" />
            <stroke android:width="1.0dip" android:color="@color/privacy_button_border" />
        </shape>
    </item>
    <item android:state_focused="false" android:state_pressed="false">
        <shape>
            <gradient android:startColor="@color/privacy_button" android:endColor="@color/privacy_button" android:angle="270.0" />
            <stroke android:width="1.0dip" android:color="@color/privacy_button_border" />
        </shape>
    </item>
</selector>